{
    "$id": "config.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "unevaluatedProperties": false,
    "description": "Cognite S3 Connector configuration file",
    "allOf": [
        {
            "$ref": "base_config.schema.json"
        }
    ],
    "properties": {
        "extractor": {
            "description": "Contains the common extractor configuration.",
            "$ref": "extractor_config.schema.json"
        },
        "destination": {
            "description": "Insert the destination configuration for DM data mirrored from CDF.",
            "$ref": "destination_config.schema.json"
        },
        "data-modeling": {
            "description": "Insert the data modeling configuration for syncing a data model to S3.",
            "type": "array",
            "items": {
                "$ref": "data_modeling_config.schema.json"
            }
        }
    }
}
